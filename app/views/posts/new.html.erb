<div class="container">
	<div class="row">
		<div class="col-md-12">
			<%= form_with model: @post do |f| %>

			<%= f.label :content %>
			<%= f.text_field :content %>
			<%= f.label :post_image %>
			<%= f.attachment_field :post_image %>

			<%= f.submit "投稿", class:"btn btn-success" %>

			<% end %>
		</div>
	</div>
</div>
